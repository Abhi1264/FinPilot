{
  "devDependencies": {
    "dotenv-cli": "^9.0.0",
    "prisma": "^6.12.0"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0"
  },
  "scripts": {
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev --schema=./prisma/schema.prisma",
    "backend": "node server/server.js",
    "db:push": "dotenv -e .env -- npx prisma db push --schema=server/prisma/schema.prisma",
    "studio": "dotenv -e .env -- npx prisma studio --schema=server/prisma/schema.prisma"
  }
}
